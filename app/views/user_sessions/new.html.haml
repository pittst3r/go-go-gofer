- content_for :asset_includes do
  = stylesheet_link_tag "user_sessions", media: "all"

= form_tag user_sessions_path, method: :post, class: "user-sessions" do
  %h1 Log In
  %p= email_field_tag :username, nil, placeholder: 'Email'
  %p= password_field_tag :password, nil, placeholder: 'Password'
  %p= submit_tag "Log In"
  %p= link_to "Forgot password?", new_password_reset_path
